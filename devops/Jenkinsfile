pipeline { 
    agent any 
    environment { 
        INSTAGRAM_NERIO = '@Nerio' 
    }
    tools {
        maven 'Maven'
    }
    options {
        skipDefaultCheckout(true)
    }

    stages {
        stage('check env') {
            steps {
                sh "mvn -v"
            }
        }
        stage('Checkout') {
            steps {
                checkout scm
            }
        }
    } 
} 